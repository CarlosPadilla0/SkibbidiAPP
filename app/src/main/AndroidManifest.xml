<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Padilla"
        tools:targetApi="31">
        <activity
            android:name=".actividad_dashboard"
            android:exported="false"
            android:label="@string/title_activity_actividad_dashboard" />
        <activity
            android:name=".actividad_transaccion"
            android:exported="false" />
        <activity
            android:name=".actividad_registro"
            android:exported="false" />
        <activity
            android:name=".ui.login.actividad_registro"
            android:exported="false"
            android:label="@string/title_activity_actividad_registro" />
        <activity
            android:name=".actividad_login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>